{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "taskName": "example-1.cpp run",
      "type": "shell",
      "command": "./a.exe",
      "dependsOn": [
        "build"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "new"
      }
    },
    {
      "taskName": "example-1.cpp build",
      "type": "shell",
      "command": "g++",
      "args": [
        "-g",
        "-std=c++11",
        "example-1.cpp"
      ],
      "isBackground": true,
      "problemMatcher": []
    }
  ]
}
